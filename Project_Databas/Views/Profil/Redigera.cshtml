@model Project_Databas.Models.ProfilDetaljer
@{
    ViewData["Title"] = "Redigera";
}

<h3>Redigera profil</h3>

<div class="row">
    <div class="col-md-4">
        <form asp-action="Redigera">
            <div asp-validation-summary="ModelOnly" class="text-danger"></div>
            <div class="form-group">
                <label asp-for="Pr_Namn" class="control-label"></label>
                <input asp-for="Pr_Namn" class="form-control" />
                <span asp-validation-for="Pr_Namn" class="text-danger"></span>
            </div>
            <div class="form-group">
                <label asp-for="Pr_Mail" class="control-label"></label>
                <input asp-for="Pr_Mail" class="form-control" />
                <span asp-validation-for="Pr_Mail" class="text-danger"></span>
            </div>
            <div class="form-group">
                <label asp-for="Pr_Bor" class="control-label"></label>
                <input asp-for="Pr_Bor" class="form-control" />
                <span asp-validation-for="Pr_Bor" class="text-danger"></span>
            </div>
            <div class="form-group">
                <label asp-for="Pr_Losenord" class="control-label"></label>
                <input asp-for="Pr_Losenord" class="form-control" />
                <span asp-validation-for="Pr_Losenord" class="text-danger"></span>
            </div>
            <div class="form-group">
                <label asp-for="Pr_Id" class="control-label"></label>
                <input asp-for="Pr_Id" class="form-control" readonly="readonly" />
                <span asp-validation-for="Pr_Id" class="text-danger"></span>
            </div>
            </br>
            <div class="form-group">
                <input type="submit" value="Spara ändringar" class="btn btn-dark" />
            </div>
        </form>
    </div>
</div>


<div>
    <a asp-action="MinProfil">Tillbaka till min profil</a>
</div>
