@model Project_Databas.Models.ProfilDetaljer

@{
    ViewData["Title"] = "Min profil";
}

<h4>Min profil</h4>

<p style="text-align:right">Du är inloggad som: @ViewBag.user</p>

<div>
    <dl class="dl-horizontal">
        <dt>
            @Html.DisplayNameFor(model => model.Pr_Namn)
        </dt>
        <dd>
            @Html.DisplayFor(model => model.Pr_Namn)
        </dd>
        <dt>
            @Html.DisplayNameFor(model => model.Pr_Mail)
        </dt>
        <dd>
            @Html.DisplayFor(model => model.Pr_Mail)
        </dd>
        <dt>
            @Html.DisplayNameFor(model => model.Pr_Losenord)
        </dt>
        <dd>
            @Html.DisplayFor(model => model.Pr_Losenord)
        </dd>
        <dt>
            @Html.DisplayNameFor(model => model.Pr_Bor)
        </dt>
        <dd>
            @Html.DisplayFor(model => model.Pr_Bor)
        </dd>
    </dl>
</div>



<div>
    @Html.ActionLink("Redigera", "Redigera", new { Pr_Mail = Model.Pr_Mail, Pr_Losenord = Model.Pr_Losenord })
    @Html.ActionLink("Visa mer", "Detaljer", new { Pr_Mail = Model.Pr_Mail, Pr_Losenord = Model.Pr_Losenord })
    @Html.ActionLink("Ta bort profil", "Radera", new { id = Model.Pr_Id })
</div>



