@model Project_Databas.Models.ProfilDetaljer

@{
    ViewData["Title"] = "Återställ lösenord";
}

<h3>Återställ lösenord</h3>


<div class="row">
    <div class="col-md-4">
        <form asp-action="Glomt">
            <div asp-validation-summary="ModelOnly" class="text-danger"></div>
            <div class="form-group">
                <label asp-for="Pr_Mail" class="control-label"></label>
                <input asp-for="Pr_Mail" class="form-control" />
                <span asp-validation-for="Pr_Mail" class="text-danger"></span>
            </div>
            </br>
            <p style="color:tomato;">@ViewBag.s</p>
            <div class="form-group">
                <input type="submit" value="Återställ lösenord" class="btn btn-dark" />
                <a asp-action="Inloggning" class="btn btn-dark">Tillbaka</a>
            </div>
        </form>
    </div>
</div>


@section Scripts {
    @{await Html.RenderPartialAsync("_ValidationScriptsPartial");}
}